{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": [
        "."
      ],
      "exclude": [
        ".git",
        ".meteor",
        "node_modules"
      ],
      "includePattern": ".+\\.js(x|doc)?$"
    },
    "opts": {
      "encoding": "utf8",
      "destination": "docs",
      "recurse": true,
      "verbose": true
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": true,
        "includeDate": true
      }
    }
  }
  